                    android:numStars="5"
                    android:rating="5"
                    android:scaleX=".7"
                    android:scaleY=".7"
                    android:theme="@style/RatingBar.Yellow"
                    android:transformPivotX="0dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/categoriesName5" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_flexGrow="1"
                app:layout_flexShrink="1">

                <ImageView
                    android:id="@+id/categoriesImage6"
                    android:layout_width="175dp"
                    android:layout_height="175dp"
                    android:layout_marginTop="8dp"
                    android:src="@drawable/gourmet6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/goSalad6"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/return2"
                    app:layout_constraintBottom_toBottomOf="@+id/ratingBar6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/categoriesName6" />

