                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/goSalad3"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="2dp"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/return2"
                    app:layout_constraintBottom_toBottomOf="@+id/ratingBar3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/categoriesName3" />

                <TextView
                    android:id="@+id/categoriesName3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/outfit_bold"
                    android:text="@string/gourmet3"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/categoriesImage3" />

                <RatingBar
                    android:id="@+id/ratingBar3"
                    style="@style/Widget.AppCompat.RatingBar.Indicator"
                    android:layout_width="wrap_content"
                    android:layout_height="43dp"
                    android:layout_marginStart="16dp"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:rating="4"
                    android:scaleX=".7"
                    android:scaleY=".7"
                    android:theme="@style/RatingBar.Yellow"
                    android:transformPivotX="0dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/categoriesName3" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
